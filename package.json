{
  "name": "@spjpgrd/inkwell",
  "version": "1.0.1",
  "description": "A gorgeous, accessible color system based on Radix Colors",
  "contributors": [
    "Colm Tuite <colm@workos.com>",
    "Vlad Moroz <vlad@workos.com>",
    "Se√°n Doran <me@spjpgrd.com>"
  ],
  "main": "index.js",
  "module": "index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "types/**/*",
    "build/**/*",
    "assets/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn && rollup -c && yarn build-css-modules",
    "build-css-modules": "node ./scripts/build-css-modules.js",
    "prepublishOnly": "yarn build",
    "postpublish": "yarn clean",
    "clean": "git clean -fdX"
  },
  "keywords": [
    "colors",
    "design-system",
    "css",
    "radix",
    "accessible",
    "inkwell"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spjpgrd/inkwell.git"
  },
  "homepage": "https://github.com/spjpgrd/inkwell#readme",
  "bugs": {
    "url": "https://github.com/spjpgrd/inkwell/issues"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^15.0.3",
    "rollup": "^2.48.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "author": "sean patrick john paul george ringo doran",
  "license": "MIT"
